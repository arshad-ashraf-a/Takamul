    <script type="text/javascript">
        function fnFillAreas(sAreaddlID, sWilayatddlID) {
            FillDropDownRoot('@Url.Action("JGetAllAreas", "Common")', 'POST', true, "ddlAreaddlID", 'AREACODE', 'AREA_NAME', '- Select Area - ', '- No data found -', [],
                function () {
                    //Success Function
                    fnFillWilayats("-99");
                    $("#ddlAreaddlID").select2({
                        width: '100%'
                    })
                    .on('change', function () {
                        $(this).closest('form').validate().element($(this));
                        fnFillWilayats($("#ddlAreaddlID").val());
                    });
                },
                function () {
                    //Error
                });

        }

        function fnFillWilayats(sAreaCode) {
            var oParameters = {};
            oParameters['sAreaCode'] = sAreaCode;

            AjaxCallAndFillCascadeDropDown('@Url.Action("JGetAllWilayat", "Common")',
                                            'Post', true,
                                            "ddAreaddlID", "ddlWilayatddlID",
                                            "WILAYATCODE", "WILLAYATNAME",
                                            "- Select Station -",
                                            "- No data found -",
                                            oParameters, true);
            $("#" + sWilayatddlID).select2({ width: '100%' })
                                .on('change', function () {
                                    $(this).closest('form').validate().element($(this));
                                    fnFillVillages($("#ddlVillageID").val());
                                });;
        }

        function fnFillVillages(sWilayatCode) {
            var oParameters = {};
            oParameters['sWilayatCode'] = sWilayatCode;
            FillDropDownRoot('@Url.Action("JGetAllVillages", "Common")', 'POST', true, "ddlVillageID", 'VILLAGECODE', 'VILLAGENAME', '- Select Area - ', '- No data found -', [],
                function () {
                    //Success Function
                    fnFillWilayats("-99");
                    $("#" + sAreaddlID).select2({
                        width: '100%'
                    })
                    .on('change', function () {
                        $(this).closest('form').validate().element($(this));
                        fnFillWilayats($("#" + sAreaddlID).val());
                    });
                },
                function () {
                    //Error
                });
        }
    </script>
